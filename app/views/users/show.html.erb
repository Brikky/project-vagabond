<h1>Welcome to Vagabond Tales, <%= @user.name %> </h1>
<div class="personal-info">
  <%= image_tag @user.profile_photo, height: '100' %>
  <h3>Current City:</h3><%= @user.current_city %>
  <h3>Name:</h3><%= @user.name %>
  <h3>email:</h3><%= @user.email %>
</div>

<div>
  <h3>Posts:</h3>

  <% @posts.each do |post| %>
  <%=link_to post.title, user_post_path(@user,post) %> <br>
  <%end%>
</div>
